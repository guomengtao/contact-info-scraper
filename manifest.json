{
  "manifest_version": 3,
  "name": "Contact Info Scraper",
  "description": "Scrapes and stores contact information from webpages",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "storage",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
} 